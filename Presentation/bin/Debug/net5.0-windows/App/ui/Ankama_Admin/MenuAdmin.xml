				 <item type="Menu" label="&lt;font color='#FFFAF0'&gt;Sanciones&lt;/font&gt;">
					   <item type="sendChat" label="Kick" command=".kick %p"/>
					   <item type="sendChat" label="Jail" command=".jail %p 999999"/>
					   <item type="sendChat" label="Ban" command=".ban %p 999999 si"/>
					   <item type="sendChat" label="UnBan" command=".unban %p"/>
					   <item type="sendChat" label="UnJail" command=".unjail %p"/>
            
                	</item>
					    <item type="sendChat" label="&lt;font color='#FFFAF0'&gt;Guardar Servidor&lt;/font&gt;" command=".saveall"/>
					    <item type="sendChat" label="&lt;font color='#FFFAF0'&gt;Añadir Estrellas a Monstruos&lt;/font&gt;" command=".monster stars 175 -all"/>
	
