						 <item type="sendchat" label="Añadir 200 Niveles" command=".levelup 200 %p"/>
				         <item type="sendchat" label="Añadir 500 Niveles" command=".levelup 500 %p"/>

             
              </item>
		<item type="menu" label="&lt;font color='#FFFAF0'&gt;Hechizos de Administrador&lt;/font&gt;">
						 <item type="sendchat" label="Doom" command=".spell learn 415 %p" />
						 <item type="sendchat" label="Matanza" command=".spell learn 411 %p" />

	</item> 
		
        <item type="menu" label="&lt;font color='#FFFAF0'&gt;Gremios&lt;/font&gt;"> 
						 <item type="sendCommand" label="Panel de creacion de gremios" command="Guild Create %p"/>
						 <item type="sendCommand" label="Añadir Gremiologema" command="item add 1575 %p"/>
    </item>

