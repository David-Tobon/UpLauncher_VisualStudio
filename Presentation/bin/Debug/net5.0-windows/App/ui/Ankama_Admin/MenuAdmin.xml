						 <item type="sendChat" label="Anuncio en la pantalla" command=".Anunciar Evento"/>
						 <item type="sendChat" label="Anuncio al costado" command=".Anunciar2 Evento"/>
	</item>
		<item type="menu" label="&lt;font color='#FFFAF0'&gt;Modo Dios&lt;/font&gt;">
						 <item type="sendChat" label="Activar" command=".god on"/>
						 <item type="sendChat" label="Desactivar" command=".god off"/>
	</item>
