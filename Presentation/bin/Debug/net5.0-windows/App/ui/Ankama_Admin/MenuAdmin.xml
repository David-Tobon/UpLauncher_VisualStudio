		                <item type="sendCommand" label="Abrir menu de monstruos (spawn)" command="monster spawn #monster" />


           </item>
		
		<item type="menu" label="&lt;font color='#FFFAF0'&gt;Niveles&lt;/font&gt;">
						 <item type="sendchat" label="Añadir 1 Nivel" command=".levelup 1 %p"/>
						 <item type="sendchat" label="Añadir 10 Niveles" command=".levelup 10 %p"/>
						 <item type="sendchat" label="Añadir 50 Niveles" command=".levelup 50 %p"/>
