		<item type="menu" label="&lt;font color='#FFFAF0'&gt;Añadir kamas&lt;/font&gt;">
						 <item type="sendChat" label="Añadir 10M Kamas" command=".kamas 10000000"/>
						 <item type="sendChat" label="Añadir 100M Kamas" command=".kamas 100000000"/>
						 <item type="sendChat" label="Añadir 500M Kamas" command=".kamas 500000000"/>
						 <item type="sendChat" label="Añadir 1B Kamas" command=".kamas 1000000000"/>
                
           </item>
		<item type="menu" label="&lt;font color='#FFFAF0'&gt;Comandos del Servidor&lt;/font&gt;">
		               	<item type="sendCommand" label="Mantenimiento" command="maintenance"  />
		               	<item type="sendCommand" label="Mantenimiento server en 1 minutos" command="shutdown 60" />
		               	<item type="sendCommand" label="Mantenimiento server en 5 minutos" command="maintenance 300"  />
